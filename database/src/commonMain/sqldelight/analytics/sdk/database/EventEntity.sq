import kotlin.String;
import kotlin.collections.Map;

-- This table stores event entities as string
CREATE TABLE IF NOT EXISTS event_entity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    eventJson TEXT AS String NOT NULL,
    propertiesMapJson TEXT AS Map<String, String> NOT NULL,
    propertiesHash TEXT NOT NULL
);

save:
INSERT INTO event_entity VALUES(?,?,?,?);

getAllByCount:
SELECT * FROM event_entity LIMIT :count;

removeByIds:
DELETE FROM event_entity WHERE id IN :ids;
